{
  "name": "vite-vue3-ts-pinia",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "eslint:comment": "使用ESLint检查并自动修复src目录下所有扩展名为.js和.vue的文件",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier:comment": "自动格式化当前目录下的所有文件",
    "prettier": "prettier --write .",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit:main": "git add . && czg && git push -u origin main:main",
    "commit:dev": "git add . && czg && git push -u origin dev:dev",
    "release": "standard-version",
    "publish": "git push --follow-tags origin dev"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "pinia": "^2.0.34",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "cz-git": "^1.6.1",
    "czg": "^1.6.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "standard-version": "^9.5.0",
    "stylelint": "^15.5.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "typescript": "^4.9.3",
    "unplugin-vue-setup-extend-plus": "^1.0.0",
    "vite": "^4.2.0",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
